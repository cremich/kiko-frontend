{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "test": "npm run test:unit",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:unit": "vue-cli-service test:unit",
    "analyze": "npm run build -- --report",
    "lint": "vue-cli-service lint",
    "codecov": "codecov -f coverage/lcov.info --disable=gcov"
  },
  "dependencies": {
    "@aws-amplify/api-graphql": "^2.0.0",
    "@aws-amplify/auth": "^4.0.0",
    "@aws-amplify/core": "^4.0.2",
    "@aws-amplify/interactions": "^4.0.0",
    "@aws-amplify/storage": "^4.0.0",
    "@aws-amplify/ui-components": "^1.3.1",
    "buefy": "^0.9.6",
    "core-js": "^3.11.1",
    "graphql-tag": "^2.12.4",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/vue": "^5.6.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.2",
    "eslint": "^7.25.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:coverage"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ],
    "*.{js,jsx,css,md,html}": "prettier --write"
  }
}
